<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:gravity="center" android:orientation="vertical">
    <LinearLayout android:id="@+id/linearLayout2" android:layout_width="296dip" android:layout_height="wrap_content">
        <LinearLayout android:id="@+id/linearLayout3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/popup_bg_top_left"></LinearLayout>
        <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout4" android:layout_width="fill_parent" android:layout_weight="1" android:background="#AEAEAE"></LinearLayout>
        <LinearLayout android:id="@+id/linearLayout5" android:layout_width="wrap_content" android:background="@drawable/popup_bg_top_right" android:layout_height="wrap_content"></LinearLayout>
    </LinearLayout>
	<LinearLayout
		android:orientation="vertical" android:layout_width="296dip"
		android:padding="0dip" android:layout_height="wrap_content" android:layout_gravity="center" android:background="#AEAEAE">
	
		<LinearLayout android:id="@+id/dialog_Title"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:paddingBottom="0dip"
			android:paddingLeft="12dip" android:paddingTop="0dip"
			android:paddingRight="12dip">
			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:minWidth="250.7dip"
				android:gravity="center" android:paddingTop="10.7dip"
				android:paddingBottom="6.7dip">
				<TextView android:layout_height="fill_parent"
					android:id="@+id/dialogAlertTitleTextView" android:gravity="center"
					android:textSize="30pt" android:layout_width="fill_parent"
					android:textColor="#171717" android:text="결제 수단 선택" />
			</LinearLayout>
		</LinearLayout>
	
		<LinearLayout android:id="@+id/select"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:paddingBottom="6.7dip"
			android:orientation="vertical" android:layout_marginLeft="2.7dip"
			android:layout_marginRight="2.7dip">
			<LinearLayout android:id="@+id/btn_select_1"
				android:layout_height="48dip" android:layout_width="fill_parent"
				android:clickable="true"
				android:background="@drawable/e_popup_list_bg">
				<TextView android:id="@+id/btn_select_1" 
					android:layout_width="fill_parent"
					android:layout_height="fill_parent" android:paddingLeft="14.7dip"
					android:singleLine="true" android:textSize="27.3pt"
					android:gravity="center"
					android:text="핸드폰 결제" android:background="#AEAEAE" android:textColor="#AEAEAE"/>
			</LinearLayout>
	
			<LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:background="@drawable/e_popup_list_line" />
	
			<LinearLayout 
				android:layout_height="48dip" android:layout_width="fill_parent"
				android:clickable="true"
				android:background="@drawable/e_popup_list_bg">
				<TextView android:id="@+id/btn_select_2" 
					android:layout_width="fill_parent"
					android:layout_height="fill_parent" android:paddingLeft="14.7dip"
					android:singleLine="true" android:textSize="27.3pt"
					android:ellipsize="end" android:gravity="center"
					android:textColor="#171717" android:text="인터넷전화 결제" />
			</LinearLayout>
		</LinearLayout>
	
		<LinearLayout  android:paddingBottom="10.7dip" android:paddingLeft="12dip" android:paddingRight="12dip"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center"
			android:paddingTop="6.7dip">
			<Button android:id="@+id/btn_ok" android:layout_width="fill_parent"
				android:layout_height="41.3dip" android:layout_weight="1"
				android:background="@drawable/popup_btn" android:textColor="#FFFFFF"
				android:textSize="28pt" android:gravity="center" android:text="취소" />
		</LinearLayout>
		
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout1" android:layout_width="296dip" android:layout_height="wrap_content">
		<LinearLayout android:id="@+id/linearLayout6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/popup_bg_botton_left"></LinearLayout>
		<LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout7" android:background="#AEAEAE" android:layout_width="fill_parent" android:layout_weight="1"></LinearLayout>
		<LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout8" android:layout_width="wrap_content" android:background="@drawable/popup_bg_botton_right"></LinearLayout>
	</LinearLayout>
</LinearLayout>